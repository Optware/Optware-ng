+#: ppdcfile.man:98
+#, no-wrap
+msgid "B<InputSlot >I<position name>"
+msgstr ""
+
+#. type: TP
+#: ppdcfile.man:100
+#, no-wrap
+msgid "B<InputSlot >I<position >B<\">I<name>B</>I<text>B<\">"
+msgstr ""
+
+#. type: TP
+#: ppdcfile.man:102
+#, no-wrap
+msgid "B<Installable >I<name>"
+msgstr ""
